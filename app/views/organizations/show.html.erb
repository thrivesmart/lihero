<% content_for :page_title do %>
organization Details - 
<% end %>
<div class="container">
<div class="page-header">
<h1>
<div class="pull-left">
<%= link_to "<span class=\"glyphicon glyphicon-step-backward\" aria-hidden=\"true\"></span>".html_safe, organizations_path, class: "btn btn-default", title: "Back to organizations" %>
&nbsp;
</div>
<div class="pull-right">
<%= link_to "<span class=\"glyphicon glyphicon-pencil\" aria-hidden=\"true\"></span> Edit".html_safe, edit_organization_path(@organization), class: "btn btn-warning" %>
</div>
organization Details
</h1>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Name</label>
  <div class="col-sm-9"><%= @organization.name %></div>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Permalink</label>
  <div class="col-sm-9"><%= @organization.permalink %></div>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Website</label>
  <div class="col-sm-9"><%= @organization.website %></div>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Creator</label>
  <div class="col-sm-9"><%= @organization.creator %></div>
</div>

</div>


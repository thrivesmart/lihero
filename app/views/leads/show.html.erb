<% content_for :page_title do %>
Lead Details - 
<% end %>
<div class="container">
	<div class="page-header">
		<h1>
			<div class="pull-left">
				<%= link_to "<span class=\"glyphicon glyphicon-step-backward\" aria-hidden=\"true\"></span>".html_safe, [@lead.list.organization, @lead.list, :leads], class: "btn btn-default", title: "Back to leads" %>
				&nbsp;
			</div>
			<div class="pull-right">
				<%= link_to "<span class=\"glyphicon glyphicon-pencil\" aria-hidden=\"true\"></span> Edit".html_safe, [:edit, @lead.list.organization, @lead.list, @lead], class: "btn btn-warning" %>
			</div>
			lead Details
		</h1>
	</div>

	<div class="row">
		<label class="col-sm-3 text-right text-muted">List</label>
		<div class="col-sm-9"><%= @lead.list %></div>
	</div>

	<div class="row">
		<label class="col-sm-3 text-right text-muted">Creator</label>
		<div class="col-sm-9"><%= @lead.creator %></div>
	</div>

	<div class="row">
		<label class="col-sm-3 text-right text-muted">Linkedinid</label>
		<div class="col-sm-9"><%= @lead.linkedinid %></div>
	</div>

	<div class="row">
		<label class="col-sm-3 text-right text-muted">Kind</label>
		<div class="col-sm-9"><%= @lead.kind %></div>
	</div>

	<div class="row">
		<label class="col-sm-3 text-right text-muted">Archived at</label>
		<div class="col-sm-9"><%= @lead.archived_at %></div>
	</div>

	<div class="row">
		<label class="col-sm-3 text-right text-muted">Name</label>
		<div class="col-sm-9"><%= @lead.name %></div>
	</div>

	<div class="row">
		<label class="col-sm-3 text-right text-muted">Email</label>
		<div class="col-sm-9"><%= @lead.email %></div>
	</div>

	<div class="row">
		<label class="col-sm-3 text-right text-muted">Phone</label>
		<div class="col-sm-9"><%= @lead.phone %></div>
	</div>

	<div class="row">
		<label class="col-sm-3 text-right text-muted">Picurl</label>
		<div class="col-sm-9"><%= @lead.picurl %></div>
	</div>

	<div class="row">
		<label class="col-sm-3 text-right text-muted">Details</label>
		<div class="col-sm-9"><%= @lead.details %></div>
	</div>

	<div class="row">
		<label class="col-sm-3 text-right text-muted">Notes</label>
		<div class="col-sm-9"><%= @lead.notes %></div>
	</div>

	<div class="row">
		<label class="col-sm-3 text-right text-muted">History</label>
		<div class="col-sm-9"><%= @lead.history %></div>
	</div>

</div>


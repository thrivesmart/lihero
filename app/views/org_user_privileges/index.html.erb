<p id="notice"><%= notice %></p>

<% content_for :page_title do %>
Org User Privileges - 
<% end %>
<div class="container">
<div class="page-header">
<h1>
<div class="pull-left">
<%= link_to "<span class=\"glyphicon glyphicon-step-backward\" aria-hidden=\"true\"></span>".html_safe, "/", class: "btn btn-default", title: "Back" %>
&nbsp;
</div>

<div class="pull-right">
<%= link_to "<span class=\"glyphicon glyphicon-plus-sign\" aria-hidden=\"true\"></span> New Org User Privilege".html_safe, [:new, :Org User Privilege], class: "btn btn-success" %>
</div>
Listing Org User Privileges
</h1>
</div>

<table class="table table-striped table-hover tablesorter" id="Org User Privileges-table">


  <thead>
<tr>
    
      <th><span>Organization</span></th>
      <th><span>User</span></th>
      <th><span>Privileges</span></th>
      <th><span>Creator</span></th>
      
    </tr>
  </thead>

  <tbody>
    <% unless @org_user_privileges.any? %>
<tr id="empty-table">
<td class="bg-warning" colspan="2">No org_user_privileges created yet.</td>
</tr>
<% end %>
<% @org_user_privileges.each do |org_user_privilege| %>
<tr class="linked-row" data-href="<%= org_user_privilege_path(org_user_privilege) %>">
      
        <td><%= link_to org_user_privilege.organization, org_user_privilege %></td>
        <td><%= link_to org_user_privilege.user, org_user_privilege %></td>
        <td><%= link_to org_user_privilege.privileges, org_user_privilege %></td>
        <td><%= link_to org_user_privilege.creator, org_user_privilege %></td>
        
        
        
      </tr>
    <% end %>
  </tbody>
</table>




